{
    "name": "beispiel",
    "version": "1.0.0",
    "description": "Beispiel mit Angular",
    "main": "echo \"Error: no main specified\" && exit 1",
    "directories": {
        "doc": "doc",
        "test": "test"
    },
    "scripts": {
        "start": "ts-node scripts/browsersync",
        "start:dev": "npx --no-install webpack-dev-server",
        "pretest": "eslint --report-unused-disable-directives test && cd ../mean-ohne-angular && npm run mongo import",
        "test": "ts-node scripts/nightwatch",
        "clean": "ts-node scripts/clean",
        "doc": "typedoc --verbose --options typedoc.json src",
        "fixme": "ts-node scripts/fixme",
        "nginx": "ts-node scripts/nginx",
        "prettier": "npx prettier --write src/**/*.ts",
        "proxy": "ts-node scripts/proxy",
        "tslint": "npx --no-install tslint src/**/*.ts",
        "prewebpack": "npx prettier --write src/**/*.ts",
        "webpack": "npx --no-install webpack --progress --profile --bail",
        "webpack:prod": "npx --no-install webpack --progress --profile --bail --config webpack.aot.config.ts",
        "prewebpack4": "npx --no-install webpack --config webpack.dll.config.ts --bail"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm test",
            "pre-push": "npm test"
        }
    },
    "private": true,
    "homepage": "https://www.hs-karlsruhe.de",
    "keywords": [
        "Angular",
        "TypeScript",
        "ES2016",
        "npm",
        "Bootstrap",
        "FontAwesome",
        "Chart.js",
        "Moment",
        "lodash",
        "Nightwatch"
    ],
    "author": "Juergen Zimmermann",
    "license": "GPL-3.0",
    "babel": {
        "plugins": [
            "add-module-exports"
        ],
        "presets": [
            [
                "env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        ]
    },
